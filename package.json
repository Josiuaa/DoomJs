{
    "name": "wolfendoomjs",
    "version": "0.5.0",
    "description": "A Wolfenstein engine running Doom game logic in the browser.",
    "keywords": [
        "Doom",
        "Wolfenstein",
        "FPS"
    ],
    "homepage": "https://github.com/yvesgurcan/jsdoom",
    "bugs": {
        "url": "https://github.com/yvesgurcan/jsdoom/issues"
    },
    "author": {
        "name": "Yves Gurcan",
        "email": "ygurcan@cbtnuggets.com",
        "url": "http://www.yvesgurcan.com"
    },
    "config": {
        "host": "localhost",
        "port": "3001"
    },
    "scripts": {
        "start": "node server",
        "dev": "npm run dev:server & npm run dev:client",
        "dev:server": "nodemon server",
        "dev:client": "webpack --mode development --watch --config client/webpack.config.js",
        "build": "npm run build:client",
        "build:client": "webpack --mode production --config client/webpack.config.js"
    },
    "devDependencies": {
        "babel-eslint": "^8.2.6",
        "eslint-config-cbtnuggets": "^7.0.5",
        "nodemon": "^1.18.3",
        "webpack": "^4.16.5",
        "webpack-cli": "^3.1.0"
    },
    "dependencies": {
        "redux": "^4.0.0",
        "uuid4": "^1.0.0"
    }
}
